# dotfiles repo
alias config="/usr/bin/git --git-dir=$HOME/.cfg/ --work-tree=$HOME"

# tmux in 256 color mode
alias tmux="tmux -2"

# matlab
alias matlab="/opt/MATLAB/R2014b/bin/matlab -nodesktop -nosplash"

# yapf
alias yapf="python3 /usr/local/bin/yapf -i"

# docformatter
alias docformatter="docformatter -i --wrap-summaries 72 --wrap-descriptions 80"
